#include<bits/stdc++.h>
#include<vector>

using namespace std;


inline void explaindeque(){
    deque<int> d;
    d.push_back(2); //{2}
    d.emplace_front(3); //3,2}
    for(auto i : d){
        cout<<i<<"  ";
        
    }cout<<endl;
    d.pop_front();
     for(auto i : d){
        cout<<i<<"  ";
        
    }cout<<endl;
    d.pop_back();
     for(auto i : d){
        cout<<i<<"  ";
        
    }cout<<endl;
    cout<<d.empty();
    cout<<endl;
    cout<<d.size()<<endl;
    
    // same as vector like d.begin() , d.end() , etc....
}
int main(){
    explaindeque();
    return 0;
}
